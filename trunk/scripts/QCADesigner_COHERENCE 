#qcadesigner-cpu -f ../data/circuits/CircuitXOR.qca -o coherence_options.txt -n 1 -t 0.001 -e COHERENCE_VECTOR -out ../data/outputs/CircuitXOR.qca
circuit_ls=`ls ../data/circuits/`
echo These files will be used for testing:
echo $circuit_ls
for i in $circuit_ls
do
	qcadesigner-cpu -f ../data/circuits/$i -o coherence_options.txt -n 1 -t 0.001 -e COHERENCE_VECTOR -out ../data/outputs/$i
	echo $i - Done.
done
echo All done.
