circuit_ls=`ls ../data/circuits/`
echo "These files will be used for testing:"
echo $circuit_ls
for i in $circuit_ls
do
	echo Simulating $i...
	../bin/qcadesigner-cuda -f ../data/circuits/$i -o ../scripts/coherence_options.txt -n 1 -t 0.001 -e COHERENCE_VECTOR -s ../data/sim_outputs/coherence/$i > ../data/sim_outputs/coherence/$i.cuda.log
	echo $i - Done.
done
echo All done.
