circuit_ls+="CircuitAND_8000.qca "
now=$(date +%s)
echo These files will be used for testing:
echo $circuit_ls
for i in $circuit_ls
do
	echo Simulating $i...
	../bin/qcadesigner-cuda -f ../data/circuits/$i -o ../scripts/coherence_options.txt -n 1 -t 0.001 -e COHERENCE_VECTOR -s ../data/sim_outputs/coherence/$i-$now > ../data/sim_outputs/coherence/$i-$now-cuda.log
	echo Done simulating $i.
	cd ../data
	@rm gnuplot/circuit
	mv -T sim_outputs/coherence/$i-$now-cuda.out gnuplot/circuit
	cd gnuplot
	gnuplot script_plot.gpi
	mv -T circuit.png $i-$now-cuda.png
	rm circuit
	
	for mail in $@
	do
		echo Emailing data to: $mail
		echo Subject: $i-$now-cuda - Simulation done.
		echo Body: $(cat ../sim_outputs/coherence/$i-$now-cuda.log)
		echo Attachment: $i-$now-cuda.png
		cat ../sim_outputs/coherence/$i-$now-cuda.log | mutt -s "$i-$now-cuda - Simulation done." -a $i-$now-cuda.png -- $mail
		#mail -s "$i-$now-cuda - Simulation done." $mail < sim_outputs/coherence/$i-$now-cuda.log
	done
done
echo All done.
