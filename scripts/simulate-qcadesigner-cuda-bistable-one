circuit_ls+="CircuitXNOR_8000.qca "
now=$(date +%s)
echo These files will be used for testing:
echo $circuit_ls
for i in $circuit_ls
do
	echo Simulating $i...
	../bin/qcadesigner-cuda -f ../data/circuits/$i -o ../scripts/bistable_options.txt -n 1 -t 0.001 -e BISTABLE -s ../data/sim_outputs/bistable/$i-$now > ../data/sim_outputs/bistable/$i-$now-cuda.log
	echo $i - Done.
done
echo All done.
